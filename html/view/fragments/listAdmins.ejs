<head>
    <meta charset="UTF-8">
    <title>Admin Page</title>
    <link rel="stylesheet" href="/admin">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

</head>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>
            Name
        </th>
        <th>
            City
        </th>
        <th>Email</th>
        <th>Admin</th>
    </tr>
    </thead>
    <tbody>
    <% for (var j = 0; j < users.length; j++){ %>
        <tr>
            <td class="align-middle">
                <a href="#" class="d-block link-dark text-decoration-none">
                    <img class="avatar" src="/avatar?src=<%=users[j].login%>&ext=<%=users[j].avatarExtension%>" alt="mdo">
                </a>
            </td>

            <th class="ThId align-middle"><%=id[j]%></th>
            <td class="align-middle"><input name="username" value="<%=users[j].login%>" class="form-control"></td>
            <td class="align-middle"><input name="city" value="<%=users[j].city%>" class="form-control"></td>
            <td class="align-middle"><input name="email" value="<%=users[j].email%>" class="form-control"></td>
            <td class="align-middle"><input name="isAdmin" value="<%=users[j].isAdmin%>" class="form-control"></td>
        </tr>
    <%}%>

    </tbody>
</table>